	<insert id="companyMemberInsert" parameterType="CompanyVo">
		INSERT INTO COMPANY(
					company_id, 
					regist_number, 
					company_name, 
					company_pw, 
					company_tel, 
					company_email, 
					ceo_name, 
					business_type, 
					company_addr,
					business_category, 
					company_birth,
					company_sales, 
					homepage, 
					company_date) 
		VALUES(#{company_id},#{regist_number},#{company_name},
				#{company_pw},#{company_tel},#{company_email},
				#{ceo_name},#{business_type},#{company_addr},
				#{business_category},#{company_birth},#{company_sales},
				#{homepage},sysdate)
	</insert>
	
	<select id="selectone" resultType="CompanyVo" parameterType="CompanyVo">
		SELECT * FROM COMPANY WHERE company_id = #{company_id} and company_pw = #{company_pw}
	</select>
	
	<select id="companyMemberIdCheck" resultType="CompanyVo" parameterType="CompanyVo">
		SELECT * FROM COMPANY WHERE COMPANY_ID = #{company_id}
	</select>
	
	<select id="companyrNumCheck" resultType="CompanyVo" parameterType="CompanyVo">
		SELECT * FROM COMPANY WHERE REGIST_NUMBER = #{regist_number}
	</select>
